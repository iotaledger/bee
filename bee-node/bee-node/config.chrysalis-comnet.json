      "statusInterval": 10,
      "milestoneSyncCount": 200
    }
  },
  "restApi": {
    "bindAddress": "/ip4/0.0.0.0/tcp/14265",
    "jwtSalt": "Bee",
    "publicRoutes": [
      "/health",
      "/api/v1/info",
      "/api/v1/tips",
      "/api/v1/messages*",
      "/api/v1/transactions*",
      "/api/v1/milestones*",
      "/api/v1/outputs*",
      "/api/v1/addresses*",
      "/api/v1/treasury",
      "/api/v1/receipts*"
    ],
    "protectedRoutes": [
      "/api/v1/*",
      "/api/plugins/*"
    ],
    "featureProofOfWork": true,
    "whiteFlagSolidificationTimeout": 2
