{
  "alias": "bee",
  "bech32Hrp": "atoi",
  "networkId": "comnet1",
  "logger": {
    "outputs": [
      {
        "name": "stdout",
        "levelFilter": "info",
        "targetFilters": [],
        "targetExclusions": [],
