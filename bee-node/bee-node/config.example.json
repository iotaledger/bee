{
  "logger": {
    "colorEnabled": true,
    "targetWidth": 42,
    "levelWidth": 5,
    "outputs": [
      {
        "name": "stdout",
        "levelFilter": "info",
        "targetFilters": [],
        "targetExclusions": []
      },
      {
        "name": "warn.log",
        "levelFilter": "warn",
        "targetFilters": [],
        "targetExclusions": []
      }
    ]
  },
  "identity": {
    "privateKey": ""
  },
  "gossip": {
    "bindAddress": "127.0.0.1:15600"
