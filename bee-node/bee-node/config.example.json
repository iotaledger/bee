      },
      {
        "publicKey": "KEY2",
        "address": "y.y.y.y:15600",
        "alias": "Peer2"
      }
    ]
  },
  "autoPeering": {
    "bindAddress": "127.0.0.1:16500",
    "entryNodes": [
      {
        "publicKey": "KEY1",
        "address": "x.x.x.x:16500"
      },
      {
        "publicKey": "KEY2",
        "address": "y.y.y.y:16500"
      }
