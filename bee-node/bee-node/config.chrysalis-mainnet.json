  "snapshot": {
    "depth": 50,
    "intervalSynced": 50,
    "intervalUnsynced": 1000,
    "fullPath": "./snapshots/mainnet/full_snapshot.bin",
    "deltaPath": "./snapshots/mainnet/delta_snapshot.bin",
