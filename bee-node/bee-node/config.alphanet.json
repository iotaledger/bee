{
  "alias": "Bee alphanet node",
  "bech32Hrp": "rms",
  "networkId": "alphanet-5",
  "logger": {
    "outputs": [
      {
        "name": "stdout",
        "levelFilter": "info",
        "targetFilters": [],
        "targetExclusions": [],
        "colorEnabled": true
      },
      {
        "name": "warn.log",
        "levelFilter": "warn",
        "targetFilters": [],
        "targetExclusions": []
      }
    ]
  },
  "network": {
    "bindAddress": "/ip4/0.0.0.0/tcp/15600",
