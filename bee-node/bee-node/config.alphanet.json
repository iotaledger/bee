        {
          "publicKey": "3df80964cc83921e08c1fa0a4f5fc05810a634da45461b2b315fcbfd62f7cab7",
          "start": 0,
          "end": 0
        },
        {
          "publicKey": "8e222ae7e2adcfb87a2984a19aad52b1979ed1472c3cb17239a73ef1d344c35a",
          "start": 0,
          "end": 0
        }
      ]
    },
    "workers": {
      "statusInterval": 10,
      "milestoneSyncCount": 200
